<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension
         point="org.knime.workbench.repository.nodes">
	<node
            category-path="/"
            factory-class="pl.edu.agh.ed.nodes.frequentPatterns.FrequentPatternExtractorNodeFactory"/>
    <node
            category-path="/"
            factory-class="pl.edu.agh.ed.nodes.frequentSequences.FrequentSequencesExtractorNodeFactory"/>
    <node
            category-path="/"
            factory-class="pl.edu.agh.ed.nodes.closedFrequentPatterns.ClosedFrequentPatternExtractorNodeFactory"/>
    <node
          deprecated="false"
          factory-class="pl.edu.agh.ed.nodes.closedFrequentSequences.ClosedFrequentSequencesExtractorNodeFactory">
    </node>
   </extension>
   
</plugin>
